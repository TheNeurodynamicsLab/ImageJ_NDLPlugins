<?xml version="1.0" encoding="UTF-8"?>
<project basedir=".." name="ImageJ">
    <property file="nbproject/nbjdk.properties"/>
    <property location="${netbeans.user}/build.properties" name="user.properties.file"/>
    <property file="${user.properties.file}"/>
    <import file="jdk.xml"/>
    <target depends="-jdk-init" name="build">
        <ant inheritall="false" target="build"/>
    </target>
    <target depends="-jdk-init" name="clean">
        <ant inheritall="false" target="clean"/>
    </target>
    <target depends="-jdk-init" name="javadocs">
        <ant inheritall="false" target="javadocs"/>
    </target>
    <target depends="-jdk-init" name="run">
        <ant inheritall="false" target="run"/>
    </target>
    <target depends="-jdk-init" name="compile-selected-files-in-plugins">
        <ant antfile="nbproject/ide-file-targets.xml" inheritall="false" target="compile-selected-files-in-plugins"/>
    </target>
</project>
